{
  "name": "@admandev/multer-ftp-storage",
  "version": "1.0.0",
  "description": "Multer storage engine for FTP",
  "license": "MIT",
  "author": "Admandev",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "multer",
    "ftp",
    "storage",
    "server",
    "engine"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --sourcemap",
    "upgrade-deps": "ncu --upgrade && yarn install"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "eslint": "^8.28.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "ncu": "^0.2.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.0",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "basic-ftp": "^5.0.5"
  },
  "peerDependencies": {
    "multer": "^1.4.5-lts.1"
  }
}
